package niagacanvas.operator;

import java.lang.annotation.Documented;
import java.lang.annotation.ElementType;
import java.lang.annotation.Inherited;
import java.lang.annotation.Retention;
import java.lang.annotation.RetentionPolicy;
import java.lang.annotation.Target;

/**
 * 
 * @author sebastian
 * 
 *         This is used to annotate the static method of a Niagarino Operator
 *         that takes an Operator instance to hand back its parameters. Used for
 *         XML creation for example. The Parameters have to be returned in an
 *         array of ParamDescription.
 *
 */
@Documented
@Target(ElementType.METHOD)
@Inherited
@Retention(RetentionPolicy.RUNTIME)
public @interface GetParamsOfInstance {

}
